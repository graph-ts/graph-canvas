{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "ES6",
    "target": "ES6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "declaration": true,
    "strict": true,
    "noImplicitAny": true,
    "allowJs": false,
    "noImplicitReturns": true
  },
  "include": [
    "src/**/*.ts"
  ]
}